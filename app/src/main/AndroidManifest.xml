<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.appspot.NeuroCleaner"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.appspot.NeuroCleaner.Activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="org.appspot.NeuroCleaner.Activity.NeuroListActivity">
        </activity>
        <service
            android:name="org.appspot.NeuroCleaner.Servise.MemoryOfficer"
            android:enabled="true"
            android:exported="true">
        </service>
        <receiver
            android:name="org.appspot.NeuroCleaner.Servise.MemorySergeant"
            android:enabled="true"
            android:exported="true">
        </receiver>
    </application>

</manifest>
